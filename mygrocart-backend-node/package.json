{
  "name": "mygrocart-backend-node",
  "version": "1.0.0",
  "description": "MyGroCart Backend API - Node.js with Express, Apollo Server, GraphQL, and Web Scraping",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:scraper": "node test_scraper.js",
    "test:scraper:standalone": "node test_scraper_standalone.js",
    "scrape:shoprite": "node -e \"require('./services/ScrapingOrchestrator').scrapeShopRite().then(console.log).catch(console.error)\"",
    "scrape:comprehensive": "node -e \"require('./services/ScrapingOrchestrator').runComprehensiveScraping().then(console.log).catch(console.error)\"",
    "scrape:status": "curl -s http://localhost:5000/api/scraping/status | jq ."
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "supermarket",
    "price-comparison",
    "web-scraping",
    "scrape-do",
    "upc",
    "grocery"
  ],
  "author": "MyGroCart Team",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.12.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "node-cron": "^3.0.3",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mygrocart/backend"
  },
  "bugs": {
    "url": "https://github.com/mygrocart/backend/issues"
  },
  "homepage": "https://github.com/mygrocart/backend#readme"
}

